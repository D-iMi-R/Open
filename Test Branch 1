for i in range(1, 100):
    count = 0
    for f in range(2, (i-1)):
        if i % f == 0:
            count += 1
    if count == 0:
        if i % 5 ==0 and i % 3 ==0:
            print str(i) + " Prime FizzBuzz"
        elif i % 3 ==0:
            print str(i) + " Prime Fizz"
        elif i % 5 ==0:
            print str(i) + " Prime Buzz"
        else:
            print str(i) + " Just Prime"
    else:
        if i % 3 == 0 and i % 5 == 0:
            print str(i) + " FizzBuzz"
        elif i % 3 == 0:
            print str(i) + " Fizz"
        elif i % 5 == 0:
            print str(i) + " Buzz"
        else:
            print i
